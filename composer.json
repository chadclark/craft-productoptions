{
    "name": "chadclark/craft-productoptions",
    "description": "Create options with price adjustments for Craft Commerce 3 products.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "product options"
    ],
    "support": {
        "docs": "https://github.com/chadclark/craft-productoptions/blob/master/README.md",
        "issues": "https://github.com/chadclark/craft-productoptions/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chad Clark",
            "homepage": "https://github.com/chadclark"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "chadclark\\productoptions\\": "src/"
        }
    },
    "extra": {
        "name": "Product Options",
        "handle": "craft-productoptions",
        "developer": "Chad Clark",
        "developerUrl": "https://github.com/chadclark",
        "documentationUrl": "https://github.com/chadclark/craft-productoptions/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/chadclark/craft-productoptions/master/CHANGELOG.md",
        "components": {
            "productoptions": "chadclark\\productoptions\\services\\ProductOptionsService"
        },
        "class": "chadclark\\productoptions\\ProductOptions"
    }
}
